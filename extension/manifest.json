{
  "manifest_version": 3,
  "name": "Download Buckets",
  "version": "2.2",
  "permissions": [
    "downloads",
    "downloads.shelf",
    "nativeMessaging",
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": { "default_popup": "options.html" },
  "options_page": "options.html",  
  "background": { "service_worker": "background.js" }
}